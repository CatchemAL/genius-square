{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "🐍 Solve 🧠",
            "type": "debugpy",
            "request": "launch",
            "module": "python.genius_square",
            "args": [
                "solve"
            ]
        },
        {
            "name": "🐍 Benchmark ⏳",
            "type": "debugpy",
            "request": "launch",
            "module": "python.genius_square",
            "args": [
                "benchmark"
            ]
        },
        {
            "name": "🐍 All Counts ✨",
            "type": "debugpy",
            "request": "launch",
            "module": "python.genius_square",
            "args": [
                "all-counts"
            ]
        },
        {
            "name": "🐍 Count 🧮",
            "type": "debugpy",
            "request": "launch",
            "module": "python.genius_square",
            "args": [
                "count"
            ]
        },
        {
            "name": "🐍 Python Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "🦀 Main",
            "type": "lldb",
            "request": "launch",
            "cargo": {
                "args": [
                    "build",
                    "--bin=genius_square",
                    "--package=genius_square"
                ],
                "filter": {
                    "name": "genius_square",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}